{
  "name": "bytic-progress",
  "version": "4.0.0",
  "description": "Common package for Galantom websites",
  "private": true,
  "author": "Gabriel Solomon",
  "license": "proprietary",
  "homepage": "https://github.com/bytic/progress",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bytic/progress.git"
  },
  "directories": {
    "test": "tests"
  },
  "style": "dist/assets/css/progress.css",
  "sass": "resources/assets/scss/progress.scss",
  "scripts": {
    "build": "mkdirp dist && webpack --mode=development --progress ",
    "build:min": "mkdirp dist && cross-env MDC_ENV=production webpack -p --mode=production --progress ",
    "watch": "yarn run build --watch",
    "dist": "yarn run build && yarn run build:min"
  },
  "suggest": {
    "@fortawesome/fontawesome-free": "^5.9"
  },
  "devDependencies": {
    "@babel/core": "^7.28.6",
    "@babel/preset-env": "^7.28.6",
    "@fortawesome/fontawesome-free": "^5.15.4",
    "babel-loader": "^8.4.1",
    "cross-env": "^5.2.1",
    "css-loader": "^3.6.0",
    "extract-text-webpack-plugin": "v4.0.0-beta.0",
    "node-sass": "^9.0.0",
    "resolve-url-loader": "^5.0.0",
    "sass-loader": "^16.0.6",
    "style-loader": "^4.0.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.104.1",
    "webpack-cli": "^6.0.1",
    "webpack-merge": "^6.0.1"
  },
  "packageManager": "yarn@4.12.0",
  "dependencies": {
    "mkdirp": "^3.0.1"
  }
}
